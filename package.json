    "framer-motion": "^12.34.3",
    "jsonwebtoken": "^9.0.3",
    "lru-cache": "^11.2.6",
    "next": "14.2.5",
    "next-auth": "^4.24.13",
    "posthog-js": "^1.356.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-email": "^5.2.8",
